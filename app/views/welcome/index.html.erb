<h1>Create a User</h1>

<%= form_with(url: create_users_path) do |f| %>
	<%= text_field_tag :name, nil, placeholder: "Your Name" %><br>
	<%= number_field_tag :age, nil, placeholder: "Your Age" %><br>
	<%= submit_tag "Create Profile" %>

<% end %>


<br>
